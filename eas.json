{
  "cli": {
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_CONFIG": "app-production.json",
        "NODE_ENV": "production"
      },
      "channel": "production"
    },
    "production-clean": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "cache": {
        "disabled": true
      },
      "env": {
        "NODE_ENV": "production",
        "APP_CONFIG": "app-production.json"
      },
      "channel": "production"
    },
    "production-with-updates": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_CONFIG": "app-production.json",
        "NODE_ENV": "production"
      },
      "channel": "production",
      "distribution": "store"
    },
    "production-with-icons": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "production-aab": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
